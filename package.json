{
  "name": "jftapi2.0",
  "version": "1.0.0",
  "description": "Development 8",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t jftapi2.0 .",
    "docker:run": "docker run -p 3000:3000 -d jftapi2.0",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop ecosystem.config.js --env production",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^3.2.3",
    "@loopback/boot": "^1.5.12",
    "@loopback/context": "^1.24.0",
    "@loopback/core": "^1.11.0",
    "@loopback/openapi-v3": "^1.10.2",
    "@loopback/repository": "^1.15.5",
    "@loopback/rest": "^1.24.0",
    "@loopback/rest-explorer": "^1.4.5",
    "@loopback/service-proxy": "^1.3.12",
    "@types/jsonwebtoken": "8.3.5",
    "@types/nodemailer": "^6.2.2",
    "@types/uuid": "^3.4.5",
    "bcryptjs": "2.4.3",
    "chartjs-plugin-datalabels": "^0.7.0",
    "convert-array-to-csv": "^1.0.9",
    "debug": "^4.1.1",
    "express": "4.17.1",
    "isemail": "3.2.0",
    "jellyutils": "file:../jellyutils",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "loopback-connector-kv-redis": "3.0.2",
    "loopback-connector-mongodb": "5.2.0",
    "loopback-connector-rest": "3.6.0",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "moment-range-split": "^1.0.1",
    "nodemailer": "^6.3.1",
    "pm2": "^4.1.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@loopback/build": "^2.1.0",
    "@loopback/testlab": "^1.9.5",
    "@types/bcryptjs": "2.4.2",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.1",
    "@types/lodash": "4.14.144",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.5",
    "concurrently": "5.0.0",
    "mocha": "^6.2.2",
    "moment-round": "^1.0.1",
    "source-map-support": "^0.5.16",
    "typescript": "~3.7.2"
  },
  "copyright.owner": "IBM Corp."
}
